{
  "name": "@gic/core",
  "version": "5.0.2",
  "description": "Gnucoop Ionic Components",
  "keywords": [
    "ionic",
    "framework",
    "stencil",
    "mobile",
    "app",
    "webapp",
    "capacitor",
    "cordova",
    "electron",
    "progressive web app",
    "pwa",
    "gnucoop",
    "autocomplete",
    "searchable",
    "select"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "jsnext:main": "dist/esm/index.mjs",
  "collection:main": "dist/collection/index.js",
  "collection": "dist/collection/collection-manifest.json",
  "types": "dist/types/interface.d.ts",
  "files": [
    "dist/",
    "css/",
    "hydrate/",
    "loader/"
  ],
  "dependencies": {
    "@ionic/core": "^5.0.0",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@stencil/core": "1.8.3",
    "@stencil/sass": "1.0.1",
    "@types/jest": "24.0.21",
    "@types/node": "12.12.3",
    "@types/puppeteer": "1.19.1",
    "aws-sdk": "^2.497.0",
    "clean-css-cli": "^4.1.11",
    "domino": "^2.1.3",
    "fs-extra": "^8.0.1",
    "jest": "24.9.0",
    "jest-cli": "24.9.0",
    "np": "^5.0.3",
    "pixelmatch": "4.0.2",
    "puppeteer": "1.20.0",
    "rollup": "1.19.4",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-virtual": "^1.0.1",
    "sass": "^1.22.9",
    "stylelint": "10.1.0",
    "stylelint-order": "3.0.1",
    "tslint": "^5.10.0",
    "tslint-ionic-rules": "0.0.21",
    "tslint-react": "^3.6.0"
  },
  "scripts": {
    "prebuild": "node scripts/patch-stencil.js",
    "build": "yarn clean && yarn build.css && stencil build --docs && yarn cdnloader",
    "build.css": "yarn css.sass && yarn css.minify",
    "build.debug": "yarn clean && stencil build --debug",
    "build.docs.json": "stencil build --docs-json dist/docs.json",
    "clean": "node scripts/clean.js",
    "cdnloader": "node scripts/copy-cdn-loader.js",
    "css.minify": "cleancss -O2 -o ./css/gic.bundle.css ./css/gic.bundle.css",
    "css.sass": "sass src/css:./css",
    "lint": "yarn lint.ts && yarn lint.sass",
    "lint.fix": "yarn lint.ts.fix && yarn lint.sass.fix",
    "lint.sass": "stylelint 'src/**/*.scss'",
    "lint.sass.fix": "yarn lint.sass -- --fix",
    "lint.ts": "tslint --project .",
    "lint.ts.fix": "tslint --project . --fix",
    "prerelease": "yarn validate && np prerelease --yolo --any-branch --tag next",
    "prerender.e2e": "node scripts/testing/prerender.js",
    "start": "yarn build.css && stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e --max-workers=2",
    "test.spec": "stencil test --spec --max-workers=2",
    "test.spec.debug": "npx --node-arg=\"--inspect-brk\" stencil test --spec",
    "test.e2e": "stencil test --e2e --max-workers=2",
    "test.screenshot": "stencil test --e2e --screenshot --screenshot-connector=scripts/screenshot/dev.js",
    "test.screenshot.ci": "stencil test --e2e --screenshot --screenshot-connector=scripts/screenshot/ci.js --ci",
    "test.watch": "jest --watch --no-cache",
    "test.treeshake": "node scripts/treeshaking.js dist/index.mjs",
    "validate": "yarn lint && yarn test && yarn build && yarn test.treeshake"
  },
  "author": "Gnucoop",
  "license": "AGPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gnucoop/gic.git"
  },
  "bugs": {
    "url": "https://github.com/gnucoop/gic/issues"
  },
  "homepage": "https://github.com/gnucoop/gic#readme",
  "jest": {
    "preset": "@stencil/core/testing"
  }
}
